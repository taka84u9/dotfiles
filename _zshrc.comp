# CoffeeScript
_cake () {
	if [ -f Cakefile ]; then
        compadd `cake | grep '^cake ' | sed -e "s/cake \([^ ]*\) .*/\1/" | grep -v '^$'`
	fi
}

compdef _cake cake


# Orig
study () {
    cd ~/code/study/$1
}

_study () {
    local -a langs
    langs=( $( find ~/code/study/* -type d -maxdepth 0 -exec basename '{}' ';') )
    compadd $langs
    return 1;
}

compdef _study study
